<article id="<%= post.layout %>-<%= post.slug %>" itemscope itemprop="blogPost">

  <h1> <%- partial('post/title', {class_name: 'article-title'}) %> </h1>


  <div class="excerpt"><p><%- post.excerpt %></p></div>  


  <div class="toc">
    <%- toc(post.content, {list_number: false}) %>
  </div>
  
  <%- post.more %>


  <div class="meta">
    <%- partial('post/date', {date_format: null}) %>
    <%- partial('post/tag') %>
  </div>


  <%- partial('post/nav') %>

</article>
